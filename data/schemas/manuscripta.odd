                        </content>-->
                        <constraintSpec ident="greek.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check that xml:lang="grc" is used for Greek characters</sch:title>
                                    <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:report test="text()[matches(., '\p{IsGreek}|\p{IsGreekExtended}')]"> Use the xml:lang="grc" for Ancient Greek text and xml:lang="el" for Modern Greek text </sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="latin.check" scheme="isoschematron">
                            <desc versionDate="2014-08-01">Checks that <att>xml:lang</att> with <val>grc</val> is not used for Latin characters</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check that xml:lang="grc" is not used for Latin characters</sch:title>
                                    <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:report test="text()[matches(., '\p{IsGreek}|\p{IsGreekExtended}')]"> Use the xml:lang="grc" for Ancient Greek text and xml:lang="el" for Modern Greek text </sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="check.forbidden.Greek.charcters" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check for forbidden Greek characters</sch:title>
                                    <sch:rule context="//*[(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:assert test="not(contains(., 'Ϛ'))"> Use Greek Small Letter Stigma (U+03DB).</sch:assert>
                                        <sch:assert test="not(contains(., 'Ϟ'))"> Use Greek Small Letter Koppa (U+03DF).</sch:assert>
                                        <sch:assert test="not(contains(., 'Ϡ'))"> Use Greek Small Letter Sampi (U+03E1).</sch:assert>
                                        <sch:assert test="not(contains(., ';'))"> Use the semicolon (U+003B) for the Greek question mark.</sch:assert>
                                        <sch:assert test="not(contains(., '·'))"> Use the middle dot (U+00B7) for the Greek semicolon.</sch:assert>
                                        <sch:assert test="not(contains(., &quot;&apos;&quot;))"> Use the right single quotation mark (U+2019) for the Greek apostrophe.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="apostrophe.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check for the apostrophe U+0027</sch:title>
                                    <sch:rule context="//*[not(@xml:lang='grc' or @xml:lang='el')]">
                                        <sch:assert test="not(contains(., &quot;&apos;&quot;))"> Use the right single quotation mark (U+2019) for apostrophe.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="quotationmark.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check for the straight double quotation mark (U+0022)</sch:title>
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., '&quot;'))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="left.quotationmark.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check for the left single quotation mark (U+2018)</sch:title>
                                    <sch:rule context="text()">
                                        <sch:assert test="not(contains(., '‘'))"> The use of quotation marks is not allowed. Use the q element instead.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <attList>
