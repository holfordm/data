                            <attDef ident="xml:id" mode="delete" />
                            <attDef ident="xml:lang" mode="delete" />
                        </attList>
                    </elementSpec>
                    <elementSpec ident="author" module="core" mode="change">
                        <content>
                            <elementRef key="persName" />
                        </content>
                        <!--<constraintSpec ident="anonymousauthor.check" scheme="isoschematron">
                            <desc versionDate="2014-09-30" xml:lang="en">checks that <gi>author</gi> only contains <q>Anonymous</q> if <gi>persName</gi> is not used</desc>
                            <constraint>
                                <sch:pattern>
                                    <sch:rule context="tei:author">
                                        <sch:assert test="text()='Anonymous' or tei:persName">The author element may only contain the value "Anonymous" when not using a persName element.</sch:assert>
                                    </sch:rule>
                                </sch:pattern>
