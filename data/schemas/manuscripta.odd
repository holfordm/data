                    <elementSpec ident="msItem" mode="change" module="msdescription">
                        <!--<content>
                            <sequence preserveOrder="true">
                                <alternate>
                                    <elementRef key="locus" minOccurs="1" maxOccurs="unbounded" />
                                    <elementRef key="locusGrp" minOccurs="1" maxOccurs="unbounded" />
                                </alternate>
                                <elementRef key="textLang" minOccurs="0" maxOccurs="1" />
                                <elementRef key="author" minOccurs="0" maxOccurs="1" />
                                <elementRef key="title" minOccurs="0" maxOccurs="1" />
                                <elementRef key="rubric" minOccurs="0" maxOccurs="1" />
                                <elementRef key="msItem" minOccurs="0" maxOccurs="unbounded" />
                                <elementRef key="incipit" minOccurs="0" maxOccurs="1" />
                                <elementRef key="explicit" minOccurs="0" maxOccurs="1" />
                                <elementRef key="finalRubric" minOccurs="0" maxOccurs="1" />
                                <elementRef key="note" minOccurs="0" maxOccurs="1" />
                                <elementRef key="colophon" minOccurs="0" maxOccurs="1" />
                                <elementRef key="filiation" minOccurs="0" maxOccurs="unbounded" />
                                <alternate>
                                    <elementRef key="listBibl" minOccurs="0" maxOccurs="1" />
                                    <elementRef key="bibl" minOccurs="0" maxOccurs="unbounded" />
                                </alternate>
                            </sequence>
                        </content>-->
                        <content>
                            <rng:group>
                                <rng:choice>
                                    <rng:ref name="locus" />
                                    <rng:ref name="locusGrp" />
                                </rng:choice>
                            </rng:group>
                            <rng:optional>
                                <rng:ref name="textLang" />
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="author" />
                            </rng:optional>
                            <rng:zeroOrMore>
                                <rng:ref name="title" />
                            </rng:zeroOrMore>
                            <rng:optional>
                                <rng:ref name="rubric" />
                            </rng:optional>
                            <rng:zeroOrMore>
                                <rng:ref name="msItem" />
                            </rng:zeroOrMore>
