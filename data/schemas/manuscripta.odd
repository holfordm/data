                            <rng:optional>
                                <rng:ref name="quote" />
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="explicit" />
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="finalRubric" />
                            </rng:optional>
                            <rng:zeroOrMore>
                                <rng:ref name="note" />
                            </rng:zeroOrMore>
