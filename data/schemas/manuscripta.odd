                    <elementSpec ident="locus" mode="change" module="msdescription">
                        <!--FIXA-->
                        <constraintSpec ident="locusGrp.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check if locus is used more than once when not inside a locusGrp</sch:title>
                                    <!--<sch:rule context="//tei:locus[not(parent::tei:locusGrp)]">
                                        <sch:report test="count(tei:locus[following::element()]) &gt; 1"> Use locusGrp when grouping together more than one locus.</sch:report>
                                    </sch:rule>-->
                                    <sch:rule context="//tei:locusGrp">
                                        <sch:report test="count(tei:locus) &lt; 2"> Only use locusGrp when grouping together more than one locus.</sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="locus.check" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check for prohibited values in the from and to attributes</sch:title>
                                    <sch:rule context="tei:locus">
                                        <sch:report test="@from[starts-with(., 'i') or starts-with(., 'v') or starts-with(., 'x')] or @to[starts-with(., 'i') or starts-with(., 'v') or starts-with(., 'x')]"> The use of roman numerals is not allowed. Use L1r for left endleaves and R1r for right endleaves.</sch:report>
                                        <sch:report test="@from[matches(., 'lco')] or @to[matches(., 'lco')]"> Use LCO for left cover outside.</sch:report>
                                        <sch:report test="@from[matches(., 'lci')] or @to[matches(., 'lci')]"> Use LCI for left cover inside.</sch:report>
                                        <sch:report test="@from[matches(., 'rci')] or @to[matches(., 'rci')]"> Use RCI for right cover inside.</sch:report>
                                        <sch:report test="@from[matches(., 'rco')] or @to[matches(., 'rco')]"> Use RCO for right cover outside.</sch:report>
                                        <sch:report test="@from[matches(., 'Spine')] or @to[matches(., 'Spine')]"> Use spine with lower-case.</sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="locus.scheme" scheme="isoschematron">
                            <constraint>
                                <sch:pattern>
                                    <sch:title>Check that the correct scheme is used for locus</sch:title>
                                    <sch:rule context="tei:locus[@scheme='pages']">
                                        <sch:report test="@from[contains(., 'r') or contains(., 'v')]"> Use folios scheme.</sch:report>
                                    </sch:rule>
                                </sch:pattern>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="from" mode="change" usage="req" />
                            <attDef ident="to" mode="change" usage="req" />
                            <attDef ident="scheme" mode="change" usage="req">
                                <valList mode="replace" type="closed">
                                    <valItem ident="folios" />
                                    <valItem ident="pages" />
                                    <valItem ident="other">
                                        <desc>Use for spine</desc>
                                    </valItem>
                                </valList>
                            </attDef>
                            <attDef ident="n" mode="delete" />
                            <attDef ident="xml:id" mode="delete" />
                            <attDef ident="xml:lang" mode="delete" />
                        </attList>
                    </elementSpec>
