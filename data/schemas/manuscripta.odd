                        </exemplum>
                    </elementSpec>
                    <elementSpec ident="bindingDesc" module="msdescription" mode="change">
                        <!--<content>
                            <sequence>
                                <elementRef key="p" minOccurs="1" maxOccurs="unbounded" />
                                <elementRef key="decoNote" minOccurs="0" maxOccurs="1" />
                                <elementRef key="condition" minOccurs="0" maxOccurs="1" />
                            </sequence>
                        </content>-->
                        <content>
                            <rng:zeroOrMore>
                                <rng:ref name="p" />
                            </rng:zeroOrMore>
                            <rng:optional>
                                <rng:ref name="decoNote" />
                            </rng:optional>
                            <rng:optional>
                                <rng:ref name="condition" />
                            </rng:optional>
                        </content>
                        <attList>
                            <attDef ident="facs" mode="delete" />
                            <attDef ident="n" mode="delete" />
                            <attDef ident="xml:id" mode="delete" />
                            <attDef ident="xml:lang" mode="delete" />
                        </attList>
                        <exemplum>
                            <egXML xmlns="http://www.tei-c.org/ns/Examples">
                                <bindingDesc>
                                    <p><term><material>Parchment</material> cover</term> over <term><material>paper</material> boards</term> with gold tooling. Sewn on three supports.</p>
                                    <p>
                                        <dimensions type="binding" unit="mm">
                                            <height quantity="230" />
                                            <width quantity="170" />
                                            <depth quantity="25" />
                                        </dimensions>
                                    </p>
                                    <decoNote>Gold-tooled <term>centerpiece</term> depicting the Crucifixion of Christ.</decoNote>
                                    <condition>There is a 20 mm tear on left cover.</condition>
                                </bindingDesc>
                            </egXML>
                        </exemplum>
                    </elementSpec>
                    <elementSpec ident="body" module="textstructure" mode="change">
                        <!--<content>
                            <elementRef key="p" minOccurs="1" maxOccurs="unbounded" />
                        </content>-->
                        <content>
                            <rng:oneOrMore>
                                <rng:ref name="p" />
                            </rng:oneOrMore>
                        </content>
                        <attList>
                            <attDef ident="n" mode="delete" />
                        </attList>
                    </elementSpec>
                    <elementSpec ident="catchwords" module="msdescription" mode="change">
                        <!--<content>
                            <interleave minOccurs="0" maxOccurs="unlimited">
                                <textNode />
                                <elementRef key="term" minOccurs="0" maxOccurs="unbounded" />
                                <elementRef key="locus" minOccurs="0" maxOccurs="unbounded" />
                           </interleave>
                        </content>-->
                        <content>
