        <fileDesc>
            <titleStmt>
                <title>Schema and Encoding Guidelines for Greek Manuscripts in Sweden</title>
                <author xml:id="PG">Patrik Granholm</author>
                <author xml:id="EN">Eva Nyström</author>
            </titleStmt>
            <publicationStmt>
                <p>Released under GNU GPL 3.0</p>
            </publicationStmt>
            <sourceDesc>
                <p>created electronically</p>
            </sourceDesc>
        </fileDesc>
