                                <rng:choice>
                                    <rng:text />
                                    <rng:ref name="term" />
                                    <rng:ref name="locus" />
                                </rng:choice>
                            </rng:zeroOrMore>
                        </content>
                        <attList>
                            <attDef ident="facs" mode="delete" />
